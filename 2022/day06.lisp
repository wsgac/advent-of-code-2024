(in-package #:advent-of-code-2022.day06)

(defun parse-input (input)
  nil)

(defun marker? (string n)
  (= n (length (delete-duplicates string))))

(defun find-start-of-marker (string n)
  (loop
    :with len := (length string)
    :for i :from 0 :below (- len n)
    :for chunk := (subseq string i (+ i n))
    :when (marker? chunk n)
      :do (return (+ i n))))

(defun problem-1 (&key (input *input-part-1-test*))
  (find-start-of-marker input 4))

(defun problem-2 (&key (input *input-part-2-test*))
  (find-start-of-marker input 14))

