(in-package #:advent-of-code-2024.day04)

(defun parse-input (input)
  (let ((rows (util:split-lines input)))
    (make-array (list (length rows) (length (first rows)))
                :initial-contents rows)))

(defun generate-deltas ()
  (loop
    for dr from -1 to 1
    append (loop
              for dc from -1 to 1
                  unless (and (zerop dr) (zerop dc))
                    collect (cons dr dc))))

(defun xmas-in-direction? (arr row col dr dc)
  (ignore-errors
   (and (char= #\M (aref arr (+ row dr) (+ col dc)))
        (char= #\A (aref arr (+ row dr dr) (+ col dc dc)))
        (char= #\S (aref arr (+ row dr dr dr) (+ col dc dc dc))))))

(defun count-xmas (arr row col)
  (loop
    for (dr . dc) in (generate-deltas)
    when (xmas-in-direction? arr row col dr dc)
      count 1))

(defun problem-1 (&key (input *input-part-1-test*))
  (let ((arr (parse-input input)))
    (loop
      for row from 0 below (array-dimension arr 0)
      sum (loop
            for col from 0 below (array-dimension arr 1)
            if (char= #\X (aref arr row col))
              sum (count-xmas arr row col)))))

(defun count-xmas-2 (arr row col)
  (or (ignore-errors
       (let ((chars '(#\M #\S)))
         (if (and (null (set-exclusive-or (list (aref arr (+ row 1) (+ col 1))
                                                (aref arr (+ row -1) (+ col -1)))
                                          chars))
                  (null (set-exclusive-or (list (aref arr (+ row -1) (+ col 1))
                                                (aref arr (+ row 1) (+ col -1)))
                                          chars)))
             1 0)))
      0))

(defun problem-2 (&key (input *input-part-2-test*))
  (let ((arr (parse-input input)))
    (loop
      for row from 1 below (1- (array-dimension arr 0))
      sum (loop
            for col from 1 below (1- (array-dimension arr 1))
            when (char= #\A (aref arr row col))
            sum (count-xmas-2 arr row col)))))

